<div class="main-container">
  <div id="<%= dom_id book %>" class="book"=>
      <h1><%= book.name %></h1>

      <strong>Autor(a):</strong>
      <%= book.author %>

      <p>
        <%= book.desc %>
      </p>

    <p>
      <strong>ISBN:</strong>
      <%= book.isbn %>
    </p>
  </div>
</div>


